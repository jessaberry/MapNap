{
  "dependencies": {
    "@aws-sdk/client-s3": "^3.363.0",
    "@material-ui/core": "^4.12.4",
    "aws-sdk": "^2.1400.0",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^3.2.0",
    "material-ui-dropzone": "^3.5.0",
    "mongoose": "^7.3.1",
    "node": "^20.2.0",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "pug": "^3.0.2",
    "react-aws-s3": "^1.5.0",
    "react-share": "^4.4.1",
    "server": "^1.0.38"
  },
  "scripts": {
    "start": "node ./server/bin/www",
    "server": "nodemon ./server/bin/www",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install": "npm i --prefix client && npm i --prefix server",
    "prettier": "npx prettier . --write --prefix client && npx prettier . --write --prefix server"
  }
}
